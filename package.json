{
  "name": "nuxt3-template",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "start": "node .output/server/index.mjs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.2.5",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@vue-macros/nuxt": "^0.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "configure-env": "^1.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@pinia/nuxt": "^0.4.11",
    "@vueuse/core": "^9.6.0",
    "@vueuse/nuxt": "^9.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "nuxt": "^3.6.5",
    "nuxt-simple-robots": "^3.0.3",
    "nuxt-simple-sitemap": "^3.0.12",
    "pinia": "^2.1.4"
  }
}
